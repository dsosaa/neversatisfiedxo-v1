# Cloudflare CSP Fix - Terraform Variables Example
# Copy this file to terraform.tfvars and fill in your values

# Option 1: API Token (Recommended)
# Create an API token at: https://dash.cloudflare.com/profile/api-tokens
# Required permissions: Zone:Zone:Read, Zone:Zone Settings:Edit
# cloudflare_api_token = "your-api-token-here"

# Option 2: Global API Key (Alternative)
# cloudflare_email   = "your-email@example.com"
# cloudflare_api_key = "your-global-api-key"

# Domain configuration (automatically detected from zone)
# domain_name = "videos.neversatisfiedxo.com"